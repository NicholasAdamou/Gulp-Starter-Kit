block mixins

block variables
	- var assets = 'assets/'
	- var css = assets + 'css/'
	- var js = assets + 'js/'
	- var images = assets + 'images/'
	- var favicons = images + 'favicons/'

doctype html
html(lang='en' class='#{pageName}')
	head
		include ../includes/partials/_head.pug
		body
			block content
			include ../includes/partials/_scripts.pug
		
